<%# resource_cache(resource) do %> 
<article class='project'>
	<div class='row-fluid row-fluid-header'>
		<div class='span12'>
	  		<%= page_title(resource.title) %> 
		</div>
	</div> 
	<div class='row-fluid row-fluid-body'>
		<div class='span7'>
		  <%= render_slideshow_for(resource) %>
		</div>
		<div class='span5'>
		  <%= render_html_text(resource.body) %>
		  <%= render_resource_categories %>
		  <%= render_resource_stickers %>
		  <%#= render_next_previous %>
		</div>
	</div> 
</article>
<%# end %>